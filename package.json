{
  "name": "study-test-es6-universal-lib",
  "version": "1.0.0",
  "description": "Studying how one would test node/browser ES6 librairies",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:node",
    "test:node": "node test/browser.js",
    "test:browser": "npm run write-zuul && zuul test/browser.js --no-coverage",
    "test-cov": "echo \"Error: no test specified\" && exit 1",
    "write-zuul": "COMMIT_SHA1=$(echo $TRAVIS_COMMIT | cut -c1-7 ) && printf \"name: $TRAVIS_BRANCH ($COMMIT_SHA1) - $TRAVIS_JOB_NUMBER\\nui: tape\\nhtml: test/basic.html\\nbrowsers:\\n  - name: $BROWSER_NAME\\n    version: $BROWSER_VERSION\\n\">.zuul.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglasduteil/study-test-es6-universal-lib.git"
  },
  "keywords": [
    "study",
    "test",
    "es6",
    "universal",
    "experimental"
  ],
  "author": {
    "name": "Douglas Duteil",
    "email": "douglasduteil@gmail.com"
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/douglasduteil/study-test-es6-universal-lib/issues"
  },
  "homepage": "https://github.com/douglasduteil/study-test-es6-universal-lib#readme",
  "devDependencies": {
    "tape": "^4.7.0",
    "zuul": "^3.11.1"
  }
}
